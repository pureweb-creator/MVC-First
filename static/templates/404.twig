{%
    include 'includes/header.twig' with
    {
        'title': page_title,
        'user': logged_user,
        'app_id': app_id,
        'root': root
    }
%}
<section class="page page_404">
    <div class="container">
        <div class="row">
            <div class="col-12 ml-auto">
                <div class="page__not-found text-center">
                    <span class="page__404-code">{{ 404 }}</span>
                    <h1 class="page__title">{{ page_title }}</h1>
                    <p class="page__subtitle">{{ page_subtitle }}</p>
                </div>
            </div>
        </div>
    </div>
</section>

{%
    include 'includes/footer.twig' with
    {
        'root': root
    }
%}